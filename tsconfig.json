{
	"compilerOptions": {
		"outDir": "out",
		"rootDir": "src",
		"baseUrl": "src",
		"module": "commonjs",
		"target": "es6",
		"strict": true,
		"noLib": true,
		"stripInternal": true,
		"downlevelIteration": true,
		"declaration": true,
		"typeRoots": ["rbx-types"]
	},
	"typeAcquisition": {
		"enable": true
	}
}