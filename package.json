{
	"name": "rbx-net",
	"version": "1.0.0-alpha.3",
	"description": "Networking Library for roblox-ts",
	"main": "./out/init.lua",
	"typings": "./out/index.d.ts",
	"scripts": {
		"test": "rbxtsc && lua ./out/Test/spec.lua",
		"prepublish": "npm run build",
		"cleanup": "rm -rf ./out && rm -rf ./dist",
		"declaration-fixes": "sh declarationfix.sh",
		"build": "npm run cleanup && rbxtsc && npm run declaration-fixes",
		"buildrbxm": "npm run cleanup && rbxtsc && npm run rbxm",
		"rbxm": "mkdir -p ./dist && rojo build --output ./dist/rbx-net.rbxm ./lua.project.json"
	},
	"author": "vorlias",
	"license": "MIT",
	"devDependencies": {
		"rbx-types": "^1.0.152"
	},
	"dependencies": {},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Vorlias/rbx-net.git"
	},
	"keywords": [
		"roblox",
		"typescript",
		"networking"
	],
	"bugs": {
		"url": "https://github.com/Vorlias/rbx-net/issues"
	},
	"homepage": "https://github.com/Vorlias/rbx-net#readme"
}
