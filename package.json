{
	"name": "rbx-net",
	"version": "0.7.0-alpha.2",
	"description": "Networking Library for roblox-ts",
	"main": "./out/init.lua",
	"typings": "./out/index.d.ts",
	"scripts": {
		"test": "rbxtsc --standalone && lua ./out/Test/spec.lua",
		"prepublish": "npm run build",
		"cleanup": "rm -rf ./out && rm -rf ./dist",
		"build": "npm run cleanup && rbxtsc",
		"buildrbxm": "npm run cleanup && rbxtsc --standalone && npm run rbxm",
		"rbxm": "mkdir -p ./dist && rojo build --output ./dist/rbx-net.rbxm ./lua.project.json"
	},
	"author": "vorlias",
	"license": "MIT",
	"devDependencies": {
		"rbx-types": "^1.0.126"
	},
	"dependencies": {},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Vorlias/rbx-net.git"
	},
	"keywords": [
		"roblox",
		"typescript",
		"networking"
	],
	"bugs": {
		"url": "https://github.com/Vorlias/rbx-net/issues"
	},
	"homepage": "https://github.com/Vorlias/rbx-net#readme"
}
